{
  "name": "trie-autocomplete-ps",
  "version": "1.1.3",
  "description": "Реализация префиксного дерева для автозаполнения с поддержкой слов и фраз",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "rollup -c",
    "prepare": "yarn build",
    "lint": "eslint \"{src,test}/**/*.ts\" --fix",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable",
    "test": "jest"
  },
  "keywords": [
    "trie",
    "autocomplete",
    "prefix-tree",
    "search",
    "typescript"
  ],
  "author": "Pavel Samoylenko <psamoylenko2@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.4",
    "@types/jest": "^30.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^9.1.7",
    "jest": "^30.0.4",
    "pinst": "^3.0.0",
    "prettier": "^3.6.2",
    "rollup": "^4.45.1",
    "terser": "^5.43.1",
    "ts-jest": "^29.4.0",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3"
  },
  "files": [
    "dist/**/*"
  ],
  "dependencies": {
    "@rollup/plugin-commonjs": "^28.0.6"
  }
}
